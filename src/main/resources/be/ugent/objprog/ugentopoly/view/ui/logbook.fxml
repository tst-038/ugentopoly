<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>

<Group fx:id="logbookRoot" layoutX="105.0" stylesheets="@../../css/logbook.css" visible="false" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.ugent.objprog.ugentopoly.controller.LogBookController">
    <children>
      <AnchorPane prefHeight="550.0" prefWidth="350.0" styleClass="anchor-pane">
         <children>
            <TableView fx:id="logTableView" maxWidth="350.0" prefHeight="500.0" prefWidth="350.0">
              <columns>
                <TableColumn fx:id="timestampColumn" styleClass="timestampColumn" editable="false" maxWidth="60.0" minWidth="60.0" prefWidth="-1.0" sortable="false" text="Tijd" />
                <TableColumn fx:id="messageColumn" styleClass="logColumn" editable="false" maxWidth="279.0" minWidth="279.0" prefWidth="-1.0" sortable="false" text="Log" />
              </columns>
            </TableView>
         </children>
         <effect>
            <DropShadow />
         </effect>
      </AnchorPane>
        <Button layoutX="14.0" layoutY="510.0" mnemonicParsing="false" onAction="#hideLogbook" styleClass="button" text="Terug" />
    </children>
</Group>
